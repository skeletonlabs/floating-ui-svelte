{
	"name": "@skeletonlabs/floating-ui-svelte",
	"version": "0.1.0",
	"license": "MIT",
	"scripts": {
		"dev": "vite dev",
		"preview": "vite preview",
		"build": "pnpm build:docs && pnpm build:package",
		"build:docs": "svelte-kit sync && vite build",
		"build:package": "svelte-kit sync && svelte-package && publint",
		"ci:publish": "pnpm build:package && changeset publish",
		"test": "vitest run --coverage",
		"test:watch": "vitest run --coverage --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"before-commit": "pnpm format && pnpm lint && pnpm check && pnpm test"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"dependencies": {
		"@floating-ui/dom": "^1.6.3",
		"@floating-ui/utils": "^0.2.2"
	},
	"peerDependencies": {
		"svelte": "^5.0.0-next.1"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.1",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "4.0.0-next.1",
		"@testing-library/jest-dom": "^6.4.2",
		"@testing-library/svelte": "^5.1.0",
		"@types/eslint": "^8.56.0",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"@vitest/coverage-v8": "^1.5.2",
		"autoprefixer": "^10.4.16",
		"csstype": "^3.1.3",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0-next.4",
		"jsdom": "^24.0.0",
		"lucide-svelte": "^0.373.0",
		"postcss": "^8.4.32",
		"postcss-load-config": "^5.0.2",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"publint": "^0.1.9",
		"shiki": "^1.3.0",
		"svelte": "5.0.0-next.125",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.3.6",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.11",
		"vitest": "^1.2.0"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module"
}
